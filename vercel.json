{

  "version": 2,

  "routes": [

    { "src": "/privacy(?:\\.html)?/?$", "dest": "/legal/privacy/" },

    { "src": "/terms(?:\\.html)?/?$", "dest": "/legal/terms/" },

    { "src": "/privacy\\.html/(.*)", "dest": "/legal/privacy/$1" },

    { "src": "/terms\\.html/(.*)", "dest": "/legal/terms/$1" }

  ]

}